#include <stdio.h>

int main() {
    int n, i, j, temp;
    scanf("%d", &n);
    int arr[n];
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    for(i = 0; i < n - 1; i++) {
        for(j = i + 1; j < n; j++) {
            if(arr[i] < arr[j]) {
                temp = arr[i];
                arr[i] = arr[j];
                arr[j] = temp;
            }
        }
    }
    
    int largest = arr[0];
    int second = -1;  
    for(i = 1; i < n; i++) {
        if(arr[i] < largest) {
            second = arr[i];
            break;
        }
    }

    printf("%d\n", second);
    return 0;
}
