#include<stdio.h>
int main()
{
    int arr[100],n,i,j,sum =0;
    printf("size of array: ");
    scanf("%d",&n);
    printf("elements of the array: ");
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    for(i=0;i<n;i++)
    {
        
        sum += arr[i];
    }
    printf("sum of array: %d",sum);
}
