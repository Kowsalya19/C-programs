#include<stdio.h>
int main()
{
    int arr[100],n,i,j,k;
    printf("size of the array: ");
    scanf("%d",&n);
    printf("elements of array: ");
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    for(i=0;i<n;i++)
    {
        for(j=i+1;j<n;j++)
        {
            if(arr[i] == arr[j])
            {
                for(k=j;k<n-1;k++)
            {
                arr[k] = arr[k+1];
            }
            n--;
            j--;
            }
        }
    }
    printf("After removing the duplicate: ");
    for(i=0;i<n;i++)
    {
        printf("%d ",arr[i]);
    }
}
