#include<stdio.h>
int main()
{
    int n,arr[100],i,j,r,temp;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    printf("no of rotate:\n");
    scanf("%d",&r);
    for(i=0;i<r;i++)
    {
      temp = arr[0];
      for(j=0;j<n-1;j++)
      {
          arr[j] = arr[j+1];
      }
      arr[n-1]=temp;
    }
    for(i=0;i<n;i++)
    {
        printf("Resultant array %d",arr[i]);
    }
    
}
